{
  "manifest_version": 2,
  "name": "URL Rat",
  "version": "1.0",
  "description": "Adds a red border to all webpages matching mozilla.org.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "urlrat@example.com"
    }
  },
  "icons": {
    "48": "icons/border-48.png",
    "96": "icons/border-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["url-rat.js"]
    }
  ]
}
